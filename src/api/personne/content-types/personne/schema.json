{
  "kind": "collectionType",
  "collectionName": "personnes",
  "info": {
    "singularName": "personne",
    "pluralName": "personnes",
    "displayName": "Personne",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nom": {
      "type": "string"
    },
    "Biographie": {
      "type": "richtext"
    },
    "Portrait": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Enfants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::personne.personne",
      "inversedBy": "Parents"
    },
    "Parents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::personne.personne",
      "inversedBy": "Enfants"
    },
    "Sexe": {
      "type": "enumeration",
      "enum": [
        "H",
        "F"
      ]
    },
    "Femmes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::personne.personne",
      "inversedBy": "Maris"
    },
    "Maris": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::personne.personne",
      "inversedBy": "Femmes"
    },
    "Notes": {
      "type": "richtext",
      "private": true
    },
    "Naissance": {
      "type": "string",
      "default": "inconnue",
      "regex": "^(?:inconnue|\\d{4}(?:-\\d{2}(?:-\\d{2})?)?)$"
    },
    "Mort": {
      "type": "string",
      "regex": "^(?:inconnue|\\d{4}(?:-\\d{2}(?:-\\d{2})?)?|)$"
    }
  }
}
